{
    "expo": {
        "name": "broken-schemes",
        "slug": "broken-schemes",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            },
            "intentFilters": [{
                "action": "VIEW",
                "data": [{
                    "scheme": "https",
                    "host": "example.com",
                    "pathPrefix": "/"
                }],
                "category": [
                    "BROWSABLE",
                    "DEFAULT"
                ]
            }]
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}